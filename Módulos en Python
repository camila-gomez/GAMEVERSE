import psycopg2
try:
    connection = psycopg2.connect(
        host = 'Localhost',
        user = 'postgres',
        password = '123456789',
        database = 'Gameverse',
        port = '5433'
    )
    
    #Tabla Player
    print("Conexión exitosa")
    cursor = connection.cursor()
    cursor.execute("select * from Player")
    rows = cursor.fetchall()
    for row in rows:
        print(row)
        
    #Tabla Team
    print("Conexión exitosa")
    cursor = connection.cursor()
